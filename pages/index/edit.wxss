.cu-list{
  overflow: hidden;
  border-radius: 30rpx;
}
.cu-list.menu>.cu-item{
  padding:0 50rpx;
  min-height: 132rpx;
  font-size: 32rpx;
}

.cu-list.menu>.cu-item .content{
  font-size: 32rpx;
}

.safe-btns{
  padding-bottom:calc(200rpx + env(safe-area-inset-bottom) / 2);
}

.foot{
  position: fixed;
  left:0;
  right:0;
  bottom:0;
  padding-bottom: calc(env(safe-area-inset-bottom) / 2);
}
.foot-btn{
  width: 66vw;
  height: 100rpx;
  line-height: 100rpx;
  font-size: 36rpx;
}



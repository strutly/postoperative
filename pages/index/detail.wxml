      </view>
    </view>
    <view class="cu-item">
      <view class="content">
        硫酸镁<text class="text-gray">(ml)</text>
      </view>
      <view class="action">
        <text class="text-main">{{detail.args.num22}}</text>
      </view>
    </view>

    <view class="cu-item">
      <view class="content">
        <text class="cuIcon-usefullfill margin-right-sm text-main"></text>其他
      </view>
    </view>
    <view class="cu-item">
      <view class="content">
        液体总量<text class="text-gray">(ml)</text>
      </view>
      <view class="action">
        <text class="text-main">{{detail.args.num23}}</text>
      </view>
    </view>
    <view class="cu-item">
      <view class="content">
        营养液泵入速度<text class="text-gray">(ml/h)</text>
      </view>
      <view class="action">
        <text class="text-green">{{detail.args.num24}}</text>
      </view>
    </view>
    <view class="cu-item">
      <view class="content">
        胰岛素用量<text class="text-gray">(IU)</text>
      </view>
      <view class="action">
        <text class="text-green">{{detail.args.num25}}</text>
      </view>
    </view>
    <view class="cu-item">
      <view class="content">
        胰岛素泵入速度<text class="text-gray">(ml/h)</text>
      </view>
      <view class="action">
        <text class="text-green">{{detail.args.num26}}</text>
      </view>
    </view>

  </view>

  <view class="safe-btns"></view>

</scroll-view>

<print detail="{{detail}}"></print>
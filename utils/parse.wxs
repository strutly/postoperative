var parse = {
  toFix2: function (value) {
    return parseFloat(value).toFixed(2)//此处2为保留两位小数
  },
  toFix1: function (value) {
    return parseFloat(value).toFixed(1)//此处1为保留一位小数
  },
  fix: function (value) {
    return parseFloat(value).toFixed(0)//此处0为取整数
  },
  int: function (value) {
    return parseInt(value)//直接取整
  }
}
module.exports = {
  toFix2: parse.toFix2,
  toFix1: parse.toFix1,
  fix: parse.fix,
  int:parse.int
}
